<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
    <sitecore>
        <settings>
            <!-- reCAPTCHA V2 keys -->
            <setting name="Foundation.ReCaptcha.V2.SiteKey" value="" />
            <setting name="Foundation.ReCaptcha.V2.Secret" value="" />
            <!-- reCAPTCHA Invisible keys -->
            <setting name="Foundation.ReCaptcha.Invisible.SiteKey" value="" />
            <setting name="Foundation.ReCaptcha.Invisible.Secret" value="" />
        </settings>
        <services>
            <configurator type="Sitecore.Foundation.ReCaptcha.Configuration.ServicesConfigurator, Sitecore.Foundation.ReCaptcha" />
        </services>
    </sitecore>
</configuration>