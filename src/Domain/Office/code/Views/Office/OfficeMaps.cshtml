@using Habitat.Office
@using Sitecore.Mvc.Presentation
@model dynamic
<input type="hidden" id="mapContextItem" name="mapContextItem" value="@RenderingContext.Current.Rendering.Item.ID"/>
<div class="map-container"></div>

@*TODO: store api key in the config file*@
@{
  var googleMapJsUrl = $"https://maps.googleapis.com/maps/api/js?key={ConfigSettings.GoogleMapApiKey}&callback=initMaps";
}
<script src="@googleMapJsUrl" async defer></script>